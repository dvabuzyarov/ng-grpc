{
  "name": "",
  "version": "0.0.0-semantically-released",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build ng-grpc --prod && ng build protoc-gen --prod && npm run copy:readme && add-shebang",
    "copy:readme": "cpx README.md ./dist/ng-grpc/ && cpx README.md ./dist/protoc-gen/",
    "build-tests": "tsc -p ./projects/ng-grpc/tsconfig.spec.json && tsc -p ./projects/protoc-gen/tsconfig.spec.json",
    "test": "npm run build-tests && node ./out-tsc/**/test.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "commit": "git-cz",
    "commit-retry": "git cz --retry",
    "semantic-release": "semantic-release"
  },
  "bin": {
    "ng-grpc-protoc-gen": "./dist/protoc-gen/bundles/ng-grpc-protoc-gen.umd.js"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "0.900.7",
    "@angular-devkit/build-ng-packagr": "0.900.7",
    "@angular/cli": "9.0.7",
    "@angular/common": "9.0.7",
    "@angular/compiler": "9.0.7",
    "@angular/compiler-cli": "9.0.7",
    "@angular/core": "9.0.7",
    "@angular/language-service": "9.0.7",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@dvabuzyarov/npm": "7.1.3",
    "@types/jasmine": "3.5.9",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.12.30",
    "add-shebang": "0.1.0",
    "codelyzer": "5.2.1",
    "cpx": "1.5.0",
    "cz-conventional-changelog": "3.1.0",
    "google-protobuf": "3.13.0",
    "grpc-web": "1.2.1",
    "husky": "4.2.3",
    "jasmine": "3.5.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "moq.ts": "5.0.5",
    "ng-auto-moq": "3.1.0",
    "ng-packagr": "9.0.3",
    "prettier": "2.0.2",
    "protoc-plugin": "0.0.6",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.4",
    "semantic-release": "17.0.4",
    "ts-node": "8.7.0",
    "tslib": "1.11.1",
    "tslint": "5.20.1",
    "typescript": "3.7.5",
    "winston": "3.2.1",
    "zone.js": "0.10.3"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@dvabuzyarov/npm",
      "@semantic-release/github"
    ],
    "pkgRoot": [
      "dist/ng-grpc",
      "dist/protoc-gen"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "commit-msg": "commitlint --edit"
    }
  }
}
